#!/var/www/tests/wt-python/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'httpie==0.8.0','console_scripts','http'
__requires__ = 'httpie==0.8.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('httpie==0.8.0', 'console_scripts', 'http')()
    )
